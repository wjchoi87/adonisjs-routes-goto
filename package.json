{
  "name": "adonisjs-routes-goto",
  "displayName": "AdonisJS Routes Goto",
  "description": "Go to Definition for AdonisJS v6 routes",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/wjchoi87/adonisjs-routes-goto"
  },
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:typescript"
  ],
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "16.x",
    "eslint": "^8.28.0",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "typescript": "^4.9.3"
  }
}
