{
  "name": "adonisjs-routes-goto",
  "displayName": "AdonisJS Routes Goto",
  "description": "Go to Definition for AdonisJS v6 routes",
  "version": "0.0.1",
  "publisher": "wjchoi87",
  "repository": {
    "type": "git",
    "url": "https://github.com/wjchoi87/adonisjs-routes-goto"
  },
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:typescript"
  ],
  "main": "./dist/extension.js",
  "imports": {
    "#controllers/*": "./app/controllers/*.js"
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsup --minify",
    "watch": "tsup --watch --sourcemap",
    "package": "vsce package",
    "pretest": "tsc -p ./ && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "dependencies": {
    "typescript": "^4.9.3"
  },
  "devDependencies": {
    "@types/node": "16.x",
    "@types/vscode": "^1.74.0",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "eslint": "^8.28.0",
    "ts-loader": "^9.5.4",
    "tsup": "^8.5.1",
    "typescript": "^4.9.3",
    "webpack": "^5.104.1",
    "webpack-cli": "^6.0.1"
  }
}
